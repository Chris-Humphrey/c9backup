        <!--Set Today's Date-->
        <script type="text/javascript">
            // Get Today's Date
            var date = new Date();
            
            var day = date.getDate();
            var month = date.getMonth() + 1;
            var year = date.getFullYear();
            
            if (month < 10) month = "0" + month;
            if (day < 10) day = "0" + day;
            
            var today = year + "-" + month + "-" + day;
            document.getElementById('theDate').value = today;
